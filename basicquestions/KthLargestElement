#include<bits/stdc++.h>
using namespace std;

int main(){
    
    int n;
    cin>>n;
    int temp;
    int *arr = new int[n];
    int k;
    cin>>k;
    for(int i=0;i<n;i++){
        
        cin>>arr[i];
    }
    
   sort(arr,arr+n,greater<int>());
    
   cout<<arr[k-1];
    
    
    return 0;
}
