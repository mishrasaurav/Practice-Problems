#include<iostream>
#include<vector>
#include<cstring>
using namespace std;

int main(){
    
    char c[1000];
    cin.getline(c,1000,'\n');
    int j=strlen(c)-1;
    for(int i=0;i<=j;i++,j--){
        if(c[i]==c[j]){
            cout<<" palindrome";
            break;
        }
        else{
            cout<<"not palindrome";
            break;
        }
    }
}
